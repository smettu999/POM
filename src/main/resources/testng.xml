<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FREE CRM Application Automation suite">
<Listeners>
	<Listener
	 class-name ="com.crm.qa.ExtentReportlistener.ExtentReporterNG"/>
</Listeners>


<parameter name="browserName" value="Chrome"></parameter>
<parameter name="url" value="http://automationpractice.com/index.php"></parameter>
  <test thread-count="1" name="Free CRM App Test Cases">
    <classes>
      <class name="com.crm.qa.testcases.LoginPageTest"/>
      <class name="com.crm.qa.testcases.HomePageTest"/>
      <class name="com.crm.qa.testcases.ContactsPageTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
